<%= form_for @post, html: {multipart: true} do |f| %>


  <div>
    <%= f.label :title, style: "display: block" %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>


    <%= f.file_field :image, class: "form-control" %>
    <%= f.submit "Save Post", class: "btn btn-success" %>
  <%end %>
